@using c18_98_m_csharp.Models.ClinicalHistories
@model List<c18_98_m_csharp.Models.ClinicalHistories.ClinicalHistoryEntry>

@{
    ViewData["Title"] = "Historia Clínica";
    var history = ViewData["history"] as ClinicalHistory;
}
<div>@history.Id</div>
@if (Model.Count == 0)
{
    <p>No hay entradas en la historia clínica</p>
}
@foreach (var entry in Model)
{
    <div>@entry.Id</div>
}
